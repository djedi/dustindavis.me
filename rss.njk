---
permalink: /blog/rss.xml
eleventyExcludeFromCollections: true
---
<?xml version="1.0" encoding="utf-8"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
  <channel>
    <title>Dustin Davis Blog</title>
    <description>Your blog description here</description>
    <link>https://dustindavis.me</link>
    <atom:link href="https://dustindavis.me/blog/rss.xml" rel="self" type="application/rss+xml" />
    {% for post in collections.posts | reverse %}
      <item>
        <title>{{ post.data.title }}</title>
        <description>{{ post.templateContent | htmlToAbsoluteUrls(post.url) | safe }}</description>
        <pubDate>{{ post.date | dateToRfc822 }}</pubDate>
        <link>https://dustindavis.me{{ post.url }}</link>
        <guid isPermaLink="true">https://dustindavis.me{{ post.url }}</guid>
      </item>
    {% endfor %}
  </channel>
</rss>
